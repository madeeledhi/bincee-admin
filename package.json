{
  "name": "bincee-admin",
  "version": "0.1.0",
  "description": "Bincee Administrative dashboard.",
  "main": "server/index.js",
  "author": "Sadain Iqrar",
  "keywords": [
    "express",
    "webpack",
    "node",
    "react",
    "redux"
  ],
  "scripts": {
    "start": "webpack --progress --config webpack.server.config.js && node server/dist/build.js",
    "fast": "UNIVERSAL_RENDERING=false babel-node server/index.js",
    "slow": "babel-node server/index.js",
    "server": "nodemon server/index.js",
    "dev": "nodemon --watch server/ --ignore '*.json' --exec UNIVERSAL_RENDERING=false babel-node --inspect server/index.js",
    "build": "webpack --progress --config webpack.server.config.js",
    "jest": "NODE_ENV=development jest --no-cache",
    "test": "NODE_ENV=development jest --watchAll --no-cache",
    "test-slow": "set NODE_ENV=development&& jest --watchAll --no-cache",
    "deploy": "./deploy.sh",
    "flow start": "flow start",
    "flow stop": "flow stop",
    "flow status": "flow status",
    "flow coverage": "flow coverage"
  },
  "jest": {
    "verbose": false,
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "es6"
    ],
    "moduleNameMapper": {
      "\\.(css|less|jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js"
    }
  },
